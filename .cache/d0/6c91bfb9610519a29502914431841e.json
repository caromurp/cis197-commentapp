mn":0},"source":"src/components/PostForm.js","original":{"line":59,"column":35}},{"generated":{"line":94,"column":1},"source":"src/components/PostForm.js","original":{"line":59,"column":0}},{"generated":{"line":96,"column":15},"source":"src/components/PostForm.js","original":{"line":63,"column":15}},{"generated":{"line":96,"column":40},"source":"src/components/PostForm.js","original":{"line":63,"column":23}},{"generated":{"line":96,"column":44},"source":"src/components/PostForm.js","original":{"line":63,"column":15}},{"name":"mapDispatchToProps","generated":{"line":96,"column":46},"source":"src/components/PostForm.js","original":{"line":63,"column":29}},{"generated":{"line":96,"column":64},"source":"src/components/PostForm.js","original":{"line":63,"column":15}},{"name":"PostForm","generated":{"line":96,"column":66},"source":"src/components/PostForm.js","original":{"line":63,"column":49}},{"generated":{"line":96,"column":74},"source":"src/components/PostForm.js","original":{"line":63,"column":15}}],"sources":{"src/components/PostForm.js":"/* eslint-disable react/jsx-filename-extension */\nimport React from 'react'\nimport { connect } from 'react-redux'\nimport { addPost } from '../actions'\n\nconst PostForm = ({ addPost }) => {\n  const [state, setState] = React.useState({\n    name: '',\n    postText: '',\n    parent: 0,\n    nest: 0,\n  })\n\n  const handleChange = e => {\n    const { value } = e.target\n    setState({\n      ...state,\n      [e.target.name]: value,\n    })\n  }\n\n  const handleSubmit = e => {\n    e.preventDefault()\n    addPost(state.name, state.postText, state.parent, state.nest)\n    setState({\n      name: '',\n      postText: '',\n      parent: 0,\n      nest: 0,\n    })\n  }\n\n  return (\n    <form\n      style={{ textAlign: 'center', fontFamily: 'Helvetica' }}\n      onSubmit={handleSubmit}\n    >\n      <input\n        type=\"text\"\n        name=\"name\"\n        placeholder=\"Enter your name\"\n        value={state.name}\n        onChange={handleChange}\n      />\n      <br></br>\n      <input\n        type=\"text\"\n        name=\"postText\"\n        placeholder=\"Write a post\"\n        value={state.postText}\n        onChange={handleChange}\n      />\n      <br></br>\n      <input type=\"submit\" disabled={!state.postText || !state.name} value=\"Submit\" />\n    </form>\n  )\n}\n\nconst mapDispatchToProps = dispatch => ({\n  addPost: (name, postText, parent, nest) => dispatch(addPost(name, postText, parent, nest)),\n})\n\nexport default connect(null, mapDispatchToProps)(PostForm)\n"},"lineCount":null}},"error":null,"hash":"30de8f9a390e2180134e124c6a292dcb","cacheData":{"env":{}}}