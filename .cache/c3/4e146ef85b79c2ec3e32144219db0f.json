 s from 'styled-components'\nimport ReplyForm from './ReplyForm'\n\nconst Post = ({ post, children }) => {\n  const [votes, setVotes] = useState(0)\n  const [isClicked, setIsClicked] = useState(false)\n\n  const handleChange = () => {\n    setIsClicked(true)\n  }\n\n  const handleUpVote = () => {\n    setVotes(votes + 1)\n  }\n\n  const handleDownVote = () => {\n    setVotes(votes - 1)\n  }\n\n  return (\n    <Thread>\n      <Single>\n        <p style={{ padding: 10, fontFamily: 'Helvetica', color: 'blue' }}>\n          {post.name}\n        </p>\n        <p style={{ padding: 10, fontFamily: 'Helvetica' }}>{post.postText}</p>\n        <div style={{ padding: 10 }}>\n          {post.nest < 2 \n            && (isClicked ? (\n              <ReplyForm parent={post} />\n            ) : (\n              <button onClick={handleChange}>Reply</button>\n            ))}\n        </div>\n        <div style={{ align: 'right' }}>\n          <button onClick={handleUpVote}>Up-vote</button>\n          <p style={{ fontFamily: 'Helvetica' }}>{votes}</p>\n          <button onClick={handleDownVote}>Down-vote</button>\n        </div>\n      </Single>\n      {children}\n    </Thread>\n  )\n}\n\nconst Thread = s.div`\n  border: 1px solid red;\n  padding: 20;\n  display: flex;\n  justify-content: space-between;\n  flex-direction: column;\n`\n\nconst Single = s.div`\n  display: flex;\n  justify-content: space-between;\n  flex-direction: row;\n`\n\nexport default Post\n"},"lineCount":null}},"error":null,"hash":"4c0f3688eba16619ed2de8e7dac0edaf","cacheData":{"env":{}}}